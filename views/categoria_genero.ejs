<%- include('partials/header') %>

<h2 class="mb-4 text-primary">Categoria más prestada por genero</h2>

<!-- Lista de préstamos -->
<ul class="list-group mb-4 shadow-sm">
  <% if (results.length > 0) { %>
    <% results.forEach(result => { %>
      <li class="list-group-item d-flex justify-content-between align-items-start flex-column flex-md-row">
        <div>
          Genero: <strong><%= result.genero %></strong> Categoría : <em>-<%= result.categoria %></em> Total : <em>"<%= result.Total %>"</em><br>
          
        </div>
       
      </li>
    <% }); %>
  <% } else { %>
    <li class="list-group-item text-muted">No hay coincidencias registrados.</li>
  <% } %>
</ul>


<%- include('partials/footer') %>
